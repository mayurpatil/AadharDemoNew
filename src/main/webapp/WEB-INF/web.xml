<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" 
                       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                       xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

  <servlet>
		<servlet-name>AadharCloud_json</servlet-name>
		<servlet-class>com.AadharCloudServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AadharCloud_json</servlet-name>
		<url-pattern>/authenticate_json/*</url-pattern>
	</servlet-mapping>
    
    	<servlet>
		<servlet-name>AadharCloudOTPJSON</servlet-name>
		<servlet-class>com.JSON_OTP</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AadharCloudOTPJSON</servlet-name>
		<url-pattern>/otpjson/*</url-pattern>
	</servlet-mapping>

  
    	<servlet>
		<servlet-name>AadharCloudKYCJSON</servlet-name>
		<servlet-class>com.JSON_eKYC</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AadharCloudKYCJSON</servlet-name>
		<url-pattern>/kycjson/*</url-pattern>
	</servlet-mapping>
  
  
	<welcome-file-list>
		<welcome-file>index1.html</welcome-file>
	</welcome-file-list>
</web-app>
